{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ProfitDistributorQueryAnswer",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "balance"
      ],
      "properties": {
        "balance": {
          "type": "object",
          "required": [
            "amount"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "claimable_profit"
      ],
      "properties": {
        "claimable_profit": {
          "type": "object",
          "required": [
            "amount"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "pool"
      ],
      "properties": {
        "pool": {
          "type": "object",
          "required": [
            "total_added"
          ],
          "properties": {
            "total_added": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "config"
      ],
      "properties": {
        "config": {
          "type": "object",
          "required": [
            "admin",
            "buttcoin",
            "contract_address",
            "pool_shares_token",
            "profit_tokens"
          ],
          "properties": {
            "admin": {
              "$ref": "#/definitions/HumanAddr"
            },
            "buttcoin": {
              "$ref": "#/definitions/SecretContract"
            },
            "contract_address": {
              "$ref": "#/definitions/HumanAddr"
            },
            "pool_shares_token": {
              "$ref": "#/definitions/SecretContract"
            },
            "profit_tokens": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/SecretContract"
              }
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "query_error"
      ],
      "properties": {
        "query_error": {
          "type": "object",
          "required": [
            "msg"
          ],
          "properties": {
            "msg": {
              "type": "string"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "SecretContract": {
      "type": "object",
      "required": [
        "address",
        "contract_hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "contract_hash": {
          "type": "string"
        }
      }
    },
    "Uint128": {
      "type": "string"
    }
  }
}

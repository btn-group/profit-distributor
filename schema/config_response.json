{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigResponse",
  "type": "object",
  "required": [
    "admin",
    "buttcoin",
    "profit_tokens"
  ],
  "properties": {
    "admin": {
      "$ref": "#/definitions/HumanAddr"
    },
    "buttcoin": {
      "$ref": "#/definitions/SecretContract"
    },
    "profit_tokens": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/SecretContract"
      }
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "SecretContract": {
      "type": "object",
      "required": [
        "address",
        "contract_hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "contract_hash": {
          "type": "string"
        }
      }
    }
  }
}
